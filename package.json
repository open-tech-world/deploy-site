  "dependencies": {
    "chalk": "^2.4.2",
    "eslint-plugin-jest": "^22.6.4",
    "inquirer": "^6.3.1",
    "jest": "^24.8.0",
    "ora": "^3.4.0"
  },
  "devDependencies": {
